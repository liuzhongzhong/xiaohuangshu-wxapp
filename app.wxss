/**app.wxss**/

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}

.container-page {
  width: 750rpx;
}

.title {
  display: flex;
  flex-direction: column;
  justify-content: center;
  width: 750rpx;
  height: 160rpx;
  background-color: #fff;
}

.title .text {
  font-size: 48rpx;
  font-weight: 600;
  z-index: 999;
  padding-left: 10rpx;
  margin-top: 68rpx;
  margin-left: 36rpx;
}

.title .line {
  width: 118rpx;
  height: 16rpx;
  background: rgba(255, 226, 0, 1);
  margin-top: -21rpx;
  z-index: 1;
  margin-left: 36rpx;
}

.under-area {
  width: 750rpx;
  height: 200rpx;
}

.under-info {
  width: 750rpx;
  height: 400rpx;
  font-size: 20rpx;
  font-weight: 400;
  color: rgba(178, 178, 178, 1);
  display: flex;
  align-items: center;
  justify-content: center;
  margin-top: -100rpx;
}

.default-info {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  height: 650rpx;
}

.default-info image {
  width: 150rpx;
  height: 150rpx;
}

.default-info text {
  font-size: 28rpx;
  color: #b2b2b2;
  margin-top: 30rpx;
}
